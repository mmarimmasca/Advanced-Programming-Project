import pandas as pd

class Dataset:
  def __init__(self):
    pass

class Reader(ABC):
  @abstractmethod
  def read(self):
    pass

class Reader_gff3(Reader):
  @staticmethod
  def read(file_name) -> Dataset: #vedere se funziona...
    with open(file_name) as df:
      dataset = pd.read_csv(df, delimiter="\t", names=["seqid", "source", "type", "start", "end", "score", "strand", "phase", "attributes"])
    return dataset

dataset = Reader_gff3.read('Homo_sapiens.GRCh38.85.gff3')
print(type(dataset)) #...e se qui ci da "Dataset" as a type
